<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	style="@style/ListItem" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:orientation="horizontal"
	android:background="#DDDDDD">
	<TextView android:id="@+id/label" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_alignParentLeft="true"
		android:textColor="#454545" android:shadowColor="#545554"
		android:padding="10dp" android:textSize="18dp" android:typeface="sans" android:layout_centerVertical="true"/>
	<TextView android:id="@+id/label1" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_alignParentRight="true"
		android:textColor="#454545" android:shadowColor="#545554" android:textStyle="bold"
		android:padding="10dp" android:textSize="20dp" android:typeface="sans" android:layout_marginRight="20dp" android:layout_centerVertical="true" />
	<ImageView android:id="@+id/image1" android:layout_width="25dp"
		android:layout_height="25dp" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
</RelativeLayout>